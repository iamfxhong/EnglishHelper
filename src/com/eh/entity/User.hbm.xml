<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.entity.User" table="eUser">
		<id name="uid" column="uid">
			<generator class="native"></generator>
		</id>
		<property name="createTime" column="createTime"></property>
		<property name="password" column="password"></property>
		<property name="nickname" column="nickname"></property>
		<property name="truename" column="truename"></property>
		<property name="headImgUrl" column="headImgUrl"></property>
		<property name="age" column="age"></property>
		<property name="status" column="status"></property>
		<property name="mobile" column="mobile"></property>
		<property name="email" column="email"></property>
	</class>
</hibernate-mapping>